window open 100,100 to 500,500, "Text", "TextEdit Example"

textedit 10,10 to 390,390, "Text", 3, "Text"
textedit add "Text", "Hello World"
textedit add "Text", "\nHallo World"

print textedit get$ "Text", 0
print textedit get$ "Text", 3

while(not instr(message$, "Quit"))
wend

window close "Text"
