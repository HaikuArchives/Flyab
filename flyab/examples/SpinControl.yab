window open 100,100 to 600,300, "MainView", "SpinControl"

spincontrol 10, 20, "sp1", "SpinControl", 0, 50, 5, "MainView"

spincontrol set "sp1", 15

REM MAIN LOOP
while(not quitting)
	msg$ = message$
	if(msg$<>"") print msg$
	
	switch msg$
		case "MainView:_QuitRequested|" 
			quitting = true 
			break
		
	end switch
wend

window close "MainView"
